{
  "manifest_version": 3,
  "name": "Form Auto-filler",
  "version": "1.0",
  "description": "Fills form data automatically",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "jquery.js" , "jquery-ui.js"]
    }
  ]
}